[DEFAULT]
keystone_url = <%= @keystone_url %>
state_path=<%= node['openstack']['ec2api']['state_path'] %>
admin_tenant_name=<%= node['openstack']['ec2api']['service_tenant'] %>
admin_password=<%= node['openstack']['ec2api']['service_password'] %>
admin_user=<%= node['openstack']['ec2api']['service_username'] %>

debug=<%= node["openstack"]["ec2api"]["debug"] %>
verbose=<%= node["openstack"]["ec2api"]["verbose"] %>
log_dir=<%= node["openstack"]["ec2api"]["log_dir"] %>
<% if node["openstack"]["ec2api"]["syslog"]["use"] %>
log_config = /etc/openstack/logging.conf
<% end %>

external_network = <%= node['openstack']['ec2api']['external_network'] %>
full_vpc_support = <%= node['openstack']['ec2api']['vpc_support'] %>
ec2_port=<%= node['openstack']['ec2api']['ec2api_port'] %>
logging_context_format_string = <%= node['openstack']['ec2api']['logging_context_file'] %>
api_paste_config=<%= node['openstack']['ec2api']['apipaste_file'] %>
ec2api_listen_port=<%= node['openstack']['ec2api']['ec2api_port'] %>

cinder_service_type=<%= node['openstack']['ec2api']['cinder_service_type'] %>

[database]
<% if @connection -%>
connection=<%= node['openstack']['ec2api']['connection'] %>
<% end -%>

[oslo_messaging_rabbit]
# The RabbitMQ userid. (string value)
rabbit_userid=<%= node["openstack"]["mq"]["ec2api"]["rabbit"]["userid"] %>
# The RabbitMQ password. (string value)
rabbit_password=<%= @mq_password %>